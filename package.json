    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "seed": "npx next node -- src/lib/db/seed.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-popover": "^1.1.14",
